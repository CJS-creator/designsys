{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@designsys/ui-ux-core": [
        "./packages/ui-ux-core/src/index.ts"
      ],
      "@designsys/ui-ux-patterns": [
        "./packages/ui-ux-patterns/src/index.ts"
      ],
      "@designsys/ui-ux-skills": [
        "./packages/ui-ux-skills/src/index.ts"
      ]
    },
    "strict": true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": true,
    "strictNullChecks": true
  }
}